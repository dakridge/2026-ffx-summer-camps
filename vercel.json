{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "framework": null,
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/camps", "destination": "/api/camps.json" }
  ]
}
